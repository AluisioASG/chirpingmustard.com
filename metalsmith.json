{
  "source": "content",
  "destination": "_out",

  "plugins": {
    "metalsmith-assets": {
      "source": "static"
    },
    "metalsmith-templates": {
      "pattern": "*.html",
      "engine": "templayed",
      "directory": "templates",
      "default": "default.html"
    },
    "metalsmith-clean-css": {
      "files": "styles/cm.com.css",
      "cleanCSS": {
        "processImport": true,
        "relativeTo": "content/styles/"
      }
    }
  }
}
